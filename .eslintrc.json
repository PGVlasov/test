{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "arrow-body-style": "off",
        "prefer-arrow-callback": "off"
      }
}
